{
  "kafka": {
    "key.deserializer": "org.apache.kafka.common.serialization.StringDeserializer",
    "value.deserializer": "org.apache.kafka.common.serialization.StringDeserializer",
    "timeout": 1,
    "bootstrap.servers": "yspetl12:6667,yspetl13:6667,yspetl14:6667,yspetl15:6667,yspetl16:6667,yspetl16:6667,yspetl18:6667,yspetl19:6667,yspetl20:6667,yspetl21:666",
    "group.id": "kafka2kafka-test",
    "auto.offset.reset": "latest",
    "max.records": 500,
    "topic": "mc_event_topic"
  },
  "queue": {
    "record.queue.size": 30,
    "offset.queue.size": 30
  },
  "hdfs": {
